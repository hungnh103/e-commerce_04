<tr>
  <td><%= admin_page params[:page], user_counter %></td>
  <td><%= user.name %></td>
  <td><%= user.email %></td>
  <td>
    <%= link_to t("show"), user_path(user) %>
    <%= link_to t("edit"), edit_user_path(user) %>
    <%= link_to t("destroy"), admin_user_path(user), method: :delete,
      data: {confirm: t("users.delete_users?")} %>
  </td>
</tr>
